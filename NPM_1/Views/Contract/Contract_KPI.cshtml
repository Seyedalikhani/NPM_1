    <br />
    <br />

    @Html.Label("Technology:")
    <select id="dropdown_technology" name="dropdown_technology">
        <option value="Select">Please Select a Technology</option>
        <option value="2G">2G</option>
        <option value="3G">3G</option>
        <option value="4G">4G</option>
    </select>
    <br />
    <br />

    @Html.Label("KPI:")
    <select id="dropdown_kpi" name="dropdown_kpi">
        <option value="Select">Please Select a KPI</option>
    </select>
    <br />
    <br />
    @Html.Label("Traffic:")
    <select id="dropdown_traffic" name="dropdown_traffic">
        <option value="Select">Please Select the Traffic</option>
        <option value="Voice">Voice</option>
        <option value="Data">Data</option>
    </select>

</div>








